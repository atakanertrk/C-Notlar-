    class Program
    {
        static void Main(string[] args)
        {
            string[] ogrenciler = new string[3] { "ali", "ayse", "ahmet" };
            foreach (string ogrenci in ogrenciler)
            {                                                                 // "ogrenci = mehmet" yazıp bütün elemanları mehmet yapamam 
                Console.WriteLine(ogrenci);
            }

            Console.ReadKey();

        }

     
    }
